tunity."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100101,"motion":400,"voiceFull":"fullvoice/section_101501/vo_full_101501-1-45","voiceFullStop":"fullvoice/section_101501/vo_full_101501-1-44"}],"textLeft":"I'd be glad to help out!"},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_101501/vo_full_101501-1-45"}]},"version":3}