[{"id":101602}],"textRight":"Hmm... Perhaps you're right."},{"autoTurnLast":1.0,"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":101602,"motion":0,"pos":0},{"cheek":1,"face":"mtn_ex_010.exp.json","id":302301,"motion":0,"pos":1},{"cheek":1,"face":"mtn_ex_010.exp.json","id":300201,"motion":0,"pos":2}],"turnChangeIn":"fadeIn"},{"chara":[{"id":101602}],"nameLeft":"Kanagi","textLeft":"Thanks for coming.","turnChangeOut":"fadeOut"},{"chara":[{"id":101602,"motion":100}],"textLeft":"I shall await your return, my Masters."},{"chara":[{"cheek":1,"face":"mtn_ex_011.exp.json","id":302301,"motion":100}],"nameCenter":"Aimi","textCenter":"For sure! I'm definitely gonna come again!"},{"chara":[{"cheek":1,"face":"mtn_ex_011.exp.json","id":300201,"motion":100}],"nameRight":"Natsuki","textRight":"Me too! I had so much fun!"},{"chara":[{"id":101602,"motion":200}],"textLeft":"Mm. Do take care on your journey home."},{"autoTurnLast":2.0,"bgm":"stop","turnChangeIn":"fadeIn"}]},"version":3}