"mtn_ex_010.exp.json","id":200301,"voiceFull":"fullvoice/section_101503/vo_full_101503-8-37","voiceFullStop":"fullvoice/section_101503/vo_full_101503-8-36"},{"cheek":0,"face":"mtn_ex_010.exp.json","id":200101,"motion":0}],"surround":"7102_townbuzz","textRight":"All right..."},{"chara":[{"cheek":0,"face":"mtn_ex_040.exp.json","id":200301,"motion":100,"voiceFull":"fullvoice/section_101503/vo_full_101503-8-38","voiceFullStop":"fullvoice/section_101503/vo_full_101503-8-37"}],"textRight":"I'm sorry for doubting you. Can we still join@your team?"},{"autoTurnLast":0.5,"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"motion":0,"pos":0},{"id":810100,"motion":0,"pos":2,"posX":40,"posY":0}],"textClear":1,"voiceFullStop":"fullvoice/section_101503/vo_full_101503-8-38"},{"select":[{"group":"group_2","textSelect":"Mogyuuu.@(You should turn them down!)"},{"group":"group_3","textSelect":"Mokyu mokyu!@(Yeah, let's work together!)"}]}],"group_2":[{"chara":[{"id":810100,"motion":100,"voice":"vo_kyube_0001_02"}],"nameRight":"userName","textRight":"Mogyuuu."},{"autoTurnLast":0.1,"voice":"stop"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"lipSynch":0,"voiceFull":"fullvoice/section_101503/vo_full_101503-8-40"}],"nameLeft":"Iroha","textLeft":"(But they're working so hard to find Mami...)","turnEffect":"thinking"},{"changeGroup":"group_4","voiceFullStop":"fullvoice/section_101503/vo_full_101503-8-40"}],"group_3":[{"chara":[{"id":810100,"motion":100,"voice":"vo_kyube_0001_16"}],"nameRight":"userName","textRight":"Mokyu mokyu!"},{"autoTurnLast":0.1,"voice":"stop"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"lipSynch":0,"voiceFull":"fullvoice/section_101503/vo_full_101503-8-42"}],"nameLeft":"Iroha","textLeft":"(Yeah, they're working so hard to find@Mami...)","turnEffect":"thinking"},{"changeGroup":"group_4","voiceFullStop":"fullvoice/section_101503/vo_full_101503-8-42"}],"group_4":[{"bgm":"bgm03_story02","chara":[{"cheek":0,"face":"mtn_ex_041.e