":111700,"pos":2}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_051.exp.json","id":111700,"motion":0},{"cheek":0,"face":"mtn_ex_050.exp.json","id":101601,"motion":0,"pos":0}],"nameLeft":"Kanagi","nameRight":"Mitama","textRight":"Ah, Riko?!"},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":101601,"motion":100}],"textLeft":"After her, Mitama!"},{"autoTurnLast":0.5,"chara":[{"cheek":0,"effect":"slideInLeftFast","face":"mtn_ex_010.exp.json","id":830900,"motion":0,"pos":1}],"turnChangeIn":"rollInFast","turnChangeOut":"rollOutFast"},{"autoTurnLast":0.2,"chara":[{"effect":"shake","id":830900}]},{"autoTurnLast":0.5,"chara":[{"effect":"slideOutRightFast","id":830900}]},{"autoTurnLast":0.5,"chara":[{"cheek":2,"effect":"slideInLeftFast","face":"mtn_ex_030.exp.json","id":303501,"motion":0,"pos":1}]},{"chara":[{"id":303501,"motion":300}],"nameCenter":"Riko","textBg":"shout","textCenter":"*Pant* *Pant* Mameji?! Where are you?!"},{"autoTurnLast":0.2,"chara":[{"id":303501,"pos":2}],"textClear":1},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":111700,"motion":0,"pos":0}],"nameLeft":"Mitama","nameRight":"Riko","textLeft":"Riko, wait!"},{"chara":[{"id":303501,"motion":200}],"textRight":"Oh no... What do I do?! Mitama, help!"},{"chara":[{"id":303501}],"textRight":"Mameji ran away..."},{"chara":[{"cheek":0,"face":"mtn_ex_000.exp.json","id":101601,"motion":200,"pos":0}],"nameLeft":"Kanagi","textLeft":"Dogs have keen hearing."},{"chara":[{"id":101601}],"textLeft":"The loud drumming must've scared him."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":101601}],"textLeft":"He probably wouldn't have gotten away if@he were properly leashed."},{"chara":[{"cheek":2,"face":"mtn_ex_050.exp.json","id":303501,"lipSynch":0,"motion":0}],"textRight":"[chara:303501:effect_emotion_surprise_0][se:7226_shock]Ah!"},{"chara":[{"cheek":2,"face":"mtn_ex_030.exp.json","id":303501}],"textRight":"It's all my fault..."},{"chara":[{"cheek":0,"face":"mtn_ex_051.exp.json","id":111700,"mot