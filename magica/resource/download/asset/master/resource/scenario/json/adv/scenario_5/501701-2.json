{"story":{"group_1":[{"bg":"black","bgm":"stop","nameLeft":"Leila's Mother","textLeft":"Hey, now! Isn't it about time?","turnChangeIn":"fadeIn","turnChangeOut":"fadeOut"},{"textLeft":"Heeey!"},{"textLeft":"Leila, did you hear me?!"},{"autoTurnLast":0.1,"bg":"bg_adv_17111.jpg","chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":800600,"motion":0,"pos":0}],"turnChangeIn":"rollInFast"},{"bgm":"bgm05_event06","chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":301301,"motion":400,"pos":2,"soulGem":0}],"nameLeft":"Leila's Mother","nameRight":"Leila","textRight":"Yes, yes, I did!","turnChangeOut":"rollOutFast"},{"chara":[{"id":800600}],"textLeft":"Did you get all your things ready?"},{"chara":[{"id":301301,"motion":200}],"textRight":"Yeah!"},{"chara":[{"id":800600}],"textLeft":"You didn't forget anything?"},{"chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":301301,"motion":1}],"textRight":"Nope, I got everything!"},{"chara":[{"id":800600}],"textLeft":"All right then, here's your lunch."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":301301}],"textRight":"Thanks!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":800600}],"textLeft":"I'll see you later!"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":301301,"motion":100}],"textRight":"See you!"},{"nameNarration":"","narration":"The Daito Apartment Complex,@Kamihama City","narrationBg":"bg_adv_17021.jpg","narrationEffect":"in","turnChangeIn":"fadeIn","turnChangeOut":"fadeOut"},{"autoTurnLast":0.1,"bg":"bg_adv_17081.jpg","chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":301401,"motion":0,"pos":1}],"narrationEffect":"out","turnChangeIn":"fadeIn"},{"chara":[{"id":301401,"lipSynch":0}],"nameCenter":"Seika","textCenter":"...","turnChangeOut":"fadeOut"},{"chara":[{"id":301401,"lipSynch":0,"motion":1}],"textCenter":"(Everything's clear... Yeah, okay.)"},{"chara":[{"id":301401,"lipSynch":0}],"textCenter":"..."},{"chara":[{"cheek":0,"face":"mtn_ex_001.exp.json","id":301401,"motion":0}],"textCenter