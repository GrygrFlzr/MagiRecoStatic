ara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100501,"motion":0,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-44","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-43"}],"textRight":"It doesn't have to be money."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100102,"motion":200,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-45","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-44"}],"textLeft":"Yeah, but still..."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100102,"lipSynch":0,"voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-45"}],"textLeft":"...[wait:1][chara:100102:lipSynch_1][chara:100102:voiceFull_fullvoice/section_101402/vo_full_101402-2-46]Hmm, okay."},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100102,"motion":0,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-47","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-46"}],"textLeft":"How about...I make you dinner?"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100501,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-48","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-47"}],"textRight":"Dinner?!"},{"autoTurnFirst":0.8,"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100501,"motion":200,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-49","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-48"}],"enableSkipAutoTurn":1,"textRight":"You serious?! AWESOME!"},{"chara":[{"cheek":0,"effect":"emotion_joy_0","face":"mtn_ex_011.exp.json","id":100501}],"se":"7222_happy"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100501,"motion":0,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-50","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-49"}],"enableSkipAutoTurn":0,"textRight":"I'll do it, I'll go! It's a deal!"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100102,"lipSynch":0,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-51","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-50"}],"textLeft":"(I guess that's all it took...)"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100501,"motion":100,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-52","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-51"}],"nameRight":"Felicia","textRight":"[textRed:Felicia Mitsuki] at your service!"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100102,"motion":100,"voiceFull":"fullvoice/section_101402/vo_full_101402-2-53","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-52"}],"textLeft":"O-oh... I'm Iroha Tamaki. Nice to meet@you, Felicia."},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_101402/vo_full_101402-2-53"}]},"version":3}